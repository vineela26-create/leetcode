import java.util.*;
class Solution {
    public int findGCD(int[] nums) {
        int gcd=1;
        Arrays.sort(nums);
        int n=nums.length;
        int a=nums[0];
        int b=nums[n-1];
        for(int i=1;i<=Math.min(a,b);i++) {
            
            if(a%i==0 && b%i==0) {
                gcd=i;
            }
        }
        return gcd;
    }
}
