class Solution {
    public int subtractProductAndSum(int n) {
        int temp=n;
        int prod=1;
        int sum=0;
        while(temp>0) {
            int digit=temp%10;
            sum=sum+digit;
            prod=prod*digit;
            temp=temp/10;
        }
        int result=prod-sum;
        return result;
        

    }
}
